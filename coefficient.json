{
  "alps" : {
    "version" : "1.0",
    "doc" : {
      "href" : "http://en.wikipedia.org/wiki/Linear_regression"
    },
    "descriptor" : [
      {
        "id" : "coefficient",
        "type" : "semantic",
        "description" : {"value" : "Linear model coefficient"},
        "semantic" : [
	  {
	    "id" : "term",
            "type" : "semantic",
            "description" : {"value" : "Corresponding term of the model"},
	    "href" : "http://alps.io/schema.org/Text"
          },
 	  {
	    "id" : "estimate",
            "type" : "semantic",
            "description" : {"value" : "Coefficient estimate"},
	    "href" : "http://alps.io/schema.org/Float"
          },
	  {
	    "id" : "stdError",
            "type" : "semantic",
            "description" : {"value" : "Standard error of the estimate"},
            "href" : "http://alps.io/schema.org/Float"
          },
          {
	    "id" : "tTest",
            "type" : "semantic",
            "description" : {"value" : "t test: Is the coefficient significantly non-zero"},
            "href" : "https://raw.github.com/standard-analytics/Schemas/master/tTest.json"
          },
	  {
	    "id" : "zTest",
            "type" : "semantic",
            "description" : {"value" : "z test: Is the coefficient significantly non-zero"},
            "href" : "https://raw.github.com/standard-analytics/Schemas/master/zTest.json"
          },
	  {
	    "id" : "contrast",
	    "type" : "semantic",
	    "description" : {"value" : "Vector of vectors of contrasts corresponding to the categorical variable at stake. Indexes are mapped corresponding to alphabetical order."},
	    "href" : "https://raw.github.com/standard-analytics/Schemas/master/matrix.json"
	  }
        ]
      }
    ]
  }
}