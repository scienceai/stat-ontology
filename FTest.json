{
    "alps" : {
        "version" : "1.0",
        "doc" : {
            "href" : "http://example.org/samples/full/doc.html"
        },
        "descriptor" : [
           {
                "id" : "FTest",
                "type" : "semantic",
                "description" : {"value" : "F statistical test: equality of two variances."},
                "semantic" : [
                    {
			"id" : "F",
