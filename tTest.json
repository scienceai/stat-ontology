{
  "alps" : {
    "version" : "1.0",
    "doc" : {
      "href" : "http://en.wikipedia.org/wiki/Student's_t-test"
    },
    "descriptor" : [
      {
        "id" : "tTest",
        "type" : "semantic",
        "description" : {"value" : "Student t statistical test."},
        "semantic" : [
          {
	    "id" : "t",
